{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-plusplus": "off",
        "no-multi-assign": "off",
        "class-methods-use-this": "off",
        "no-trailing-spaces": "error",
        "semi": "error",
        "linebreak-style": [
            "error",
            "unix"
        ]
    }
}